<h1>Edit Profile</h1>
<div>
  <%= simple_form_for @user do |f| %>
      <%= f.input :first_name %>
      <%= f.input :last_name %>
      <%= f.input :email %>
      <%= f.input :password %>
      <%= f.input :password_confirmation %>

      <%= f.submit 'Update', method: :update %>
  <% end %>
</div>
